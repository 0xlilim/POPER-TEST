{
    "family": "php-poper",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
      {
        "name": "php-poper-test",
        "image": "poper:latest",
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
          }
        ],
        "environment": [
        ],
        "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/php-poper",
                    "mode": "non-blocking",
                    "awslogs-create-group": "true",
                    "max-buffer-size": "25m",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-stream-prefix": "php-fpm"
                },
                "secretOptions": []
            },
        "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost/up || exit 1"
                ],
                "interval": 15,
                "timeout": 5,
                "retries": 3,
                "startPeriod": 60
            }
      }
    ],
    "executionRoleArn": "arn:aws:iam::476114136251:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::476114136251:role/ecsTaskRole"
  }