{
    "family": "php-poper",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
      {
        "name": "php-poper-test",
        "image": "poper:latest",
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
          }
        ],
        "environment": [
          {
            "name": "POPER_CUSTOM_VARIABLE",
            "value": "WOWOWOWOW www!"
          }
        ]
      }
    ],
    "executionRoleArn": "arn:aws:iam::your-account-id:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::your-account-id:role/ecsTaskRole"
  }